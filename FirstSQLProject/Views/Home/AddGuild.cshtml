
@{
    ViewBag.Title = "AddGuild";
}

<h2>Add Guild</h2>
<hr />

<form action="/Home/AddGuildSubmit" method="post">
    <table class="table">
        <tr>
            <td>
                Name:
            </td>
            <td>
                <input type="text" name="Name" class="form-control" placeholder="Type guild name here..." />
            </td>
        </tr>
        <tr>
            <td colspan="2">
                <input type="submit" value="Save" />
            </td>
        </tr>
    </table>
</form>
